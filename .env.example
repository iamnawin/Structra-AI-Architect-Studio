# Required: Gemini AI API key from Google AI Studio
GEMINI_API_KEY=your_gemini_api_key_here

# The URL where the app is hosted
APP_URL=http://localhost:3000

# Express API port
API_PORT=8787

# AI provider: "mock" (no cost, offline) or "gemini" (real AI calls)
AI_PROVIDER=mock

# Node environment
NODE_ENV=development
